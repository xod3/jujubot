{
  "manifest_version": 3,
  "name": "Full Cookie Extractor",
  "version": "1.0",
  "permissions": ["cookies", "storage", "activeTab", "scripting"],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
